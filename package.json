{
  "name": "dewu-mock-api",
  "version": "1.0.0",
  "description": "Mock API server for Dewu (得物) platform interfaces",
  "main": "dist/server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node --project tsconfig.server.json src/server/app.ts",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "node scripts/build-server.js",
    "build:client": "vite build",
    "vercel-build": "cd src/client && npx vite build --outDir ../../dist/client --emptyOutDir",
    "start": "node dist/server/app.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "dewu",
    "mock",
    "api",
    "react",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "jsdom": "^27.0.1",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^3.2.4"
  }
}
