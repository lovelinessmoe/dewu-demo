{
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/client",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/standalone"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/standalone"
    },
    {
      "source": "/dop/(.*)",
      "destination": "/api/standalone"
    }
  ]
}