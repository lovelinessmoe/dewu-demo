{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/client",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/index"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/dop/(.*)",
      "destination": "/api/index"
    }
  ]
}